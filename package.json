{
  "name": "naary",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run build:json && next build",
    "build:json": "pnpm run build:blogs && pnpm run build:rss && pnpm run build:about && pnpm run build:feed && pnpm run build:sitemap",
    "build:rss": "ts-node --project tsconfig.builder.json ./builder/rss.ts",
    "build:feed": "ts-node --project tsconfig.builder.json ./builder/feed.ts",
    "build:about": "ts-node --project tsconfig.builder.json ./builder/about.ts",
    "build:blogs": "ts-node --project tsconfig.builder.json ./builder/blogs.ts",
    "build:sitemap": "ts-node --project tsconfig.builder.json ./builder/sitemap.ts",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}' && next lint --fix"
  },
  "dependencies": {
    "@kuma-ui/core": "^1.5.2",
    "@vanilla-extract/css": "^1.13.0",
    "base64url": "^3.0.1",
    "dayjs": "^1.11.10",
    "dompurify": "^3.0.6",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "he": "^1.2.0",
    "html-react-parser": "^5.0.4",
    "jsdom": "^22.1.0",
    "microcms-js-sdk": "^2.7.0",
    "next": "13.5.6",
    "open-graph-scraper": "^6.3.2",
    "react": "^18",
    "react-dom": "^18",
    "react-html-parser": "^2.0.2",
    "rss": "^1.2.2",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.7.2",
    "@kuma-ui/next-plugin": "^1.1.12",
    "@types/dompurify": "^3.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/fs-extra": "^11.0.3",
    "@types/he": "^1.2.2",
    "@types/htmlparser2": "^3.10.6",
    "@types/jsdom": "^21.1.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-html-parser": "^2.0.4",
    "@types/rss": "^0.0.31",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
